@model IEnumerable<Microsoft.AspNetCore.Identity.IdentityUser>
@{
    ViewData["Title"] = "Users";
}

<h1>Users</h1>


<table class="table">
    <thead>
        <tr>
        <th>UserName</th>
        <th>Email</th>
        <th>PhoneNumber</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var users in Model)
        {
         <tr>
             <td>
                 @users.UserName
             </td>
             <td>
                 @users.Email
             </td>
             <td>
                 @users.PhoneNumber
             </td>
             <td>
                <a class="btn btn-primary" asp-action="Edit" asp-route-id="@users.Id">Edit</a>
                <a class="btn btn-secondary" asp-action="Delete" asp-route-id="@users.Id">Delete</a>
             </td>
         </tr>   
        }
    </tbody>
</table>